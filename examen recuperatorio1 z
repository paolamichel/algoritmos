#include <iostream>
#include <conio.h>
#include <stdio.h>
using namespace std;
void cargar(int v[],int n){
	int i;
	for(i=0;i<n;i++){
		cout<<"ingrese el numero:";
		cin>>v[i];
	}
}
void mostrar(int v[],int n){
	int i;
	for(i=0;i<n;i++){
		cout<<" "<<v[i];
	}
}
bool fibbo(int n){
bool k=false;
int a=-1,b=1,c=0;
	while(c<n){
	c=a+b;
	a=b;
	b=c;
	}
	if(c==n){
		k=true;
	}
	return(k);
}
bool primo(int n){
	bool k=true;
	int i;
		for(i=2;i<n;i++){
			if(n%i==0){
				k=false;
		}
	}
	return(k);
}
void eliminarfibboprimo(int v[],int &n){
	int i,j;
	for(i=0;i<n;i++){
		if(primo(v[i])==true || fibbo(v[i])==true){
			for(j=i;j<n-1;j++){
				v[j]=v[j+1];

			}
			i--;
			n--;
		}

	}
	
}
void main(){
int v[100];
int n;
cout<<"ingrese el tamaÃ±o de n:";
cin>>n;
cargar(v,n);
mostrar(v,n);
cout<<endl;
eliminarfibboprimo(v,n);
mostrar(v,n);
getch();
}
